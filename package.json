{
  "name": "node_express_typescript_boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for node express api project with typescript",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build-ts": "tsc",
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "watch-node": "npm run watch-ts && nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "knex",
    "bookshelf",
    "typescript",
    "bookshelf",
    "boilerplate",
    "tslint"
  ],
  "author": "a7urag",
  "license": "ISC",
  "devDependencies": {
    "@types/bookshelf": "^0.13.0",
    "nodemon": "^1.18.10",
    "tslint": "^5.14.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "http-status-codes": "^1.3.0",
    "morgan": "^1.9.1",
    "winston": "^3.2.1",
    "knex": "^0.16.3",
    "mysql": "^2.16.0",
    "bookshelf": "^0.14.2"
  }
}
